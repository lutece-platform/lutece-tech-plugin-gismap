<?xml version="1.0" encoding="UTF-8"?>
<plug-in>
    <name>gismap</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.0</version>
    <documentation></documentation>
    <installation></installation>
    <changes></changes>
    <user-guide></user-guide>
    <description>gismap.plugin.description</description>
    <provider>gismap.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright (c) 2016 Your Company</copyright>
    <db-pool-required>1</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>4.1.0</min-core-version>
        <max-core-version/>	
    </core-version-dependency>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>GISMAP_MANAGEMENT</feature-id>
            <feature-title>gismap.adminFeature.ManageGismap.name</feature-title>
            <feature-description>gismap.adminFeature.ManageGismap.description</feature-description>
            <feature-level>0</feature-level>
            <feature-url>jsp/admin/plugins/gismap/ManageHtlmViews.jsp</feature-url>
            <feature-icon-url></feature-icon-url>
      </admin-feature>
    </admin-features>    

    <!-- XPages parameters -->
    <applications>
        <application>
            <application-id>htlmview</application-id>
            <application-class>fr.paris.lutece.plugins.gismap.web.HtlmViewXPage</application-class>
        </application>
    </applications>

    <!-- Portlet parameters -->
    <portlets>
   </portlets>
</plug-in>